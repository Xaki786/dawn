{{ 'component-image-with-text.css' | asset_url | stylesheet_tag }}
<h1>{{ section.settings.title }}</h1>
{%- for block in section.blocks -%}
  <img src={{ block.settings.image.src }} height="200" width="200" loading="lazy" />
{%- endfor -%}
{% schema %}
{
    "name": "Wada Lullahh",
    "tag": "section",
    "class": "slideshow",
    "settings": [
      {
        "type": "text",
        "id": "title",
        "label": "Slideshow"
      }
    ],
    "blocks": [
       {
         "name": "Slide",
         "type": "slide",
         "settings": [
           {
             "type": "image_picker",
             "id": "image",
             "label": "Image"
           }
         ]
       }
     ]
  }
{% endschema %}